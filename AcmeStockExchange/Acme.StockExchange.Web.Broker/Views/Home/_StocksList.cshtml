<table id="stocksTable" class="display">
    <thead>
        <tr>
            <th></th>
            <th>Ticker symbol</th>
            <th>Offering amount</th>
            <th>Offering price [€]</th>
            <th></th>
        </tr>
    </thead>
    <tbody data-bind="foreach: Stocks">
        <tr>
            <td>
                <button class="btn btn-danger btn-xs" data-bind="click: $parent.DeleteStock">
                    <span class="glyphicon glyphicon-trash"></span> delete
                </button>
            </td>
            <td data-bind="text: TickerSymbol"></td>
            <td data-bind="text: OfferingAmount"></td>
            <td data-bind="text: OfferingPrice"></td>
            <td>
                <button class="btn btn-info btn-xs" data-bind="click: $parent.EditStock">
                    <span class="glyphicon glyphicon-pencil"></span> edit
                </button>
            </td>
        </tr>
    </tbody>
</table>